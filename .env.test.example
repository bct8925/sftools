# Salesforce Test Environment Credentials
# Copy this file to .env.test and fill in your values

# Access token from a Salesforce org
SF_ACCESS_TOKEN=your_access_token_here

# Instance URL (e.g., https://yourorg.my.salesforce.com)
SF_INSTANCE_URL=https://yourorg.my.salesforce.com

# Optional: Enable debug output
# DEBUG=true

# Optional: Run tests in headed mode (show browser)
# HEADED=true
