{
  "manifest_version": 3,
  "name": "Salesforce Developer Console",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["callback.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "key": ""
}